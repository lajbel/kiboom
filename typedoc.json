{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./dist/kiboom.d.ts",
    "./dist/global.d.ts"
  ],
  "out": "./docs",
  "sortEntryPoints": false,
  "plugin": [
    "typedoc-plugin-merge-modules"
  ],
  "categorizeByGroup": true,
  "navigation": {
    "includeGroups": true
  },
  "groupOrder": [
    "Functions",
    "Type Aliases",
    "Classes",
    "Interfaces"
  ],
  "mergeModulesRenameDefaults": true,
  "mergeModulesMergeMode": "project",
  "excludeCategories": [
    "Internal"
  ]
}
